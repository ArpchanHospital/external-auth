<?xml version="1.0" encoding="UTF-8"?>

<module configVersion="1.2">

	<!-- Module Properties -->
	<id>externalauth</id>
	<name>External Authentication Module</name>
	<version>1.0-SNAPSHOT</version>
	<package>org.openmrs.module.externalauth</package>
	<author>Bahmni</author>
	<description>
		Helps in adding filter for external authentication
	</description>
	<updateURL>https://dev.openmrs.org/modules/download/externalauth/update.rdf</updateURL>
